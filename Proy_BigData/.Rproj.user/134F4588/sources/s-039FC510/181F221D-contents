rm(etiq)

library(writexl)
getwd()
setwd("C:\\Users\\Dell\\Documents\\GitHub\\mi_proyecto1_est_383\\doc_y_bd\\feminicidio\\bd")
write_xlsx(var_etiq,"etiquetas de las variables casadas.xlsx")

